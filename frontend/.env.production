# Environment Variables for Frontend Production

# API Configuration
REACT_APP_API_URL=/api/v1

# Authentication
REACT_APP_AUTH_STORAGE_KEY=cafe-sundus_management_auth

# Default Language
REACT_APP_DEFAULT_LANGUAGE=ar

# Feature Flags
REACT_APP_ENABLE_DARK_MODE=true
REACT_APP_ENABLE_NOTIFICATIONS=true
REACT_APP_ENABLE_ONLINE_ORDER=true

# Build Configuration
GENERATE_SOURCEMAP=false
# Disable ESLint plugin in production build to avoid style errors failing the build
DISABLE_ESLINT_PLUGIN=true